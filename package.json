{
  "name": "Sample",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "build-css": "node-sass-chokidar src/ -o src/",
    "watch-css": "npm run build-css && node-sass-chokidar src/ -o src/ --watch --recursive",
    "start-js": "react-scripts start",
    "start": "npm-run-all -p watch-css start-js",
    "build": "npm run build-css && npm run lint && react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject",
    "lint": "node node_modules/eslint/bin/eslint.js ./src",
    "lintwatch": "node node_modules/eslint-watch/bin/esw -w ./src"
  },
  "dependencies": {
    "bootstrap": "^4.0.0-beta.2",
    "classnames": "2.2.5",
    "isomorphic-fetch": "2.2.1",
    "node-sass-chokidar": "0.0.3",
    "react": "^16.2.0",
    "react-bootstrap-table": "4.3.1",
    "react-click-outside": "3.0.1",
    "react-dom": "16.2.0",
    "react-redux": "^5.0.6",
    "react-redux-i18n": "1.9.1",
    "react-router-breadcrumbs-hoc": "1.1.1",
    "react-router-dom": "^4.2.2",
    "react-scripts": "1.0.14",
    "react-select": "1.2.1",
    "react-toastify": "3.2.1",
    "react-window-resize-listener": "1.1.0",
    "react-window-size-listener": "1.0.10",
    "reactstrap": "5.0.0-alpha.4",
    "redux": "3.7.2",
    "redux-form": "7.2.0",
    "redux-saga": "^0.16.2",
    "redux-thunk": "2.2.0",
    "reselect": "3.0.1"
  },
  "devDependencies": {
    "babel-eslint": "^8.2.6",
    "enzyme": "3.1.0",
    "enzyme-adapter-react-16": "1.0.1",
    "eslint": "4.9.0",
    "eslint-config-airbnb": "16.0.0",
    "eslint-plugin-import": "2.7.0",
    "eslint-plugin-jest": "21.2.0",
    "eslint-plugin-jsx-a11y": "6.0.2",
    "eslint-plugin-react": "7.4.0",
    "eslint-watch": "3.1.2",
    "fetch-mock": "5.13.1",
    "npm-run-all": "4.1.1",
    "react-test-renderer": "^16.5.2",
    "redux-mock-store": "1.3.0"
  }
}
